<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gxl.bluetooth.activity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".seekbar.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".seekbar.LineChartActivity"/>
        <activity android:name=".seekbar.LineChart2Activity"/>
        <activity android:name=".linechart.CloseOpenActivity"/>
        <activity android:name=".seekbar.barchart.LineChart3Activity"/>
        <activity android:name=".scatterchart.ScatterActivity"/>
        <activity android:name=".seekbar.barchart.BarChartActivity"/>
    </application>

</manifest>